<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/graphmenu"
    >

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="To Graph 1"
        android:id="@+id/buttonToGraph1"
        android:tag="1"
        android:onClick="changeActivity"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="To Graph 2"
        android:id="@+id/buttonToGraphTwo"
        android:tag="2"
        android:onClick="changeActivity"
        android:layout_below="@+id/buttonToGraph1"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="To Calender"
        android:id="@+id/calender"
        android:tag="3"
        android:onClick="changeActivity"
        android:layout_below="@+id/buttonToGraphTwo"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        />
</LinearLayout>
